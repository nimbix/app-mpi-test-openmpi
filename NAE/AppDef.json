{
  "name": "MPI Test - Open MPI",
  "classifications": [
    "Software Development/Software Testing"
  ],
  "description": "Benchmark and validation tests to demonstrate working Open MPI libraries over available fabrics and providers",
  "author": "Nimbix, Inc.",
  "vault-types": [
    "FILE",
    "BLOCK"
  ],
  "licensed": true,
  "machines": [
    "n[3-9]*",
    "n[1-9][0-9]*"
  ],
  "commands": {
    "Validate": {
      "path": "/usr/local/bin/validate.sh",
      "description": "Simple script to validate a working Open MPI library, showing basic information",
      "name": "Open MPI validation",
      "interactive": false,
      "parameters": {
      }
    },
    "Benchmarks": {
      "path": "/usr/local/bin/benchmark.sh",
      "description": "Run benchmarks to demonstrate working Open MPI libraries, fabric comparison and performance",
      "name": "Open MPI benchmarks",
      "interactive": false,
      "parameters": {
        "-bench": {
          "name": "Benchmark selection",
          "required": true,
          "type": "selection",
          "values": [
            "Startup: osu_hello",
            "Point to Point: osu_latency",
            "One-sided: osu_get_latency",
            "One-sided: osu_get_bw"
          ],
          "mvalues": [
            "startup/osu_hello",
            "pt2pt/osu_latency",
            "one-sided/osu_get_latency",
            "one-sided/osu_get_bw"
          ]
        }
      }
    },
    "Server": {
      "path": "/sbin/init",
      "interactive": true,
      "publicip": true,
      "name": "Server",
      "description": "Launch a session with all boot services, including SSH (if installed)",
      "parameters": {}
    }
  },
  "image": {
    "type": "image/png",
    "data": ""
  },
  "variables": {
  }
}
